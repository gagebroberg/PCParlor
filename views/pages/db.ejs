<!DOCTYPE html>
<html>
<head>
  <%- include ("../partials/header.ejs") %>
</head>

<body>

<%- include ("../partials/nav.ejs") %>

<div class="container">
<h2>Database Results</h2>

<!-- Outputs the content of the results var passed to the file from index.js as a list -->
<ul>
    <% results.forEach(function(r) { %>
        <li><%= r.type %> - <%= r.number %> - <%= r.brand %> - <%= r.model %> - <%= r.rank %> - <%= r.benchmark %></li>
    <% }); %>
</ul>

</div>

</body>
</html>
